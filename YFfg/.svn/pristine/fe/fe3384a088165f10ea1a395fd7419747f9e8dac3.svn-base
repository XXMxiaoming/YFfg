package com.yfwl.yfgp.model;

import java.util.Date;

public class ChatRoom {
	
	private Integer chatRoomId;
	private String easeCrId;
	private String crName;
	private String crDesc;
	private String crOwner;
	private Integer maxUser;
	private Date createTime;
	
	public ChatRoom() {
		super();
	}

	public ChatRoom(Integer chatRoomId, String easeCrId, String crName,
			String crDesc, String crOwner, Integer maxUser, Date createTime) {
		super();
		this.chatRoomId = chatRoomId;
		this.easeCrId = easeCrId;
		this.crName = crName;
		this.crDesc = crDesc;
		this.crOwner = crOwner;
		this.maxUser = maxUser;
		this.createTime = createTime;
	}

	public ChatRoom(String easeCrId, String crName, String crDesc,
			String crOwner, Integer maxUser) {
		super();
		this.easeCrId = easeCrId;
		this.crName = crName;
		this.crDesc = crDesc;
		this.crOwner = crOwner;
		this.maxUser = maxUser;
	}
	
	public ChatRoom(String easeCrId, String crName, String crDesc,
			Integer maxUser) {
		super();
		this.easeCrId = easeCrId;
		this.crName = crName;
		this.crDesc = crDesc;
		this.maxUser = maxUser;
	}

	public Integer getChatRoomId() {
		return chatRoomId;
	}
	public void setChatRoomId(Integer chatRoomId) {
		this.chatRoomId = chatRoomId;
	}
	public String getEaseCrId() {
		return easeCrId;
	}
	public void setEaseCrId(String easeCrId) {
		this.easeCrId = easeCrId;
	}
	public String getCrName() {
		return crName;
	}
	public void setCrName(String crName) {
		this.crName = crName;
	}
	public String getCrDesc() {
		return crDesc;
	}
	public void setCrDesc(String crDesc) {
		this.crDesc = crDesc;
	}
	public String getCrOwner() {
		return crOwner;
	}
	public void setCrOwner(String crOwner) {
		this.crOwner = crOwner;
	}
	public Integer getMaxUser() {
		return maxUser;
	}
	public void setMaxUser(Integer maxUser) {
		this.maxUser = maxUser;
	}
	public Date getCreateTime() {
		return createTime;
	}
	public void setCreateTime(Date createTime) {
		this.createTime = createTime;
	}
	
	
}
