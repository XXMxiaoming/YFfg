package com.yfwl.yfgp.model;

import java.util.Date;

import com.fasterxml.jackson.annotation.JsonFormat;

public class Praise {
	private Integer praiseId;
	private Integer userId;
	private Integer praiseContent;
	private Integer contentType;
	private Integer praiseState;
	private Date praiseTime;
	
	public void setPraiseId(Integer praiseId) {
		this.praiseId = praiseId;
	}
	
	public Integer getPraiseId(){
		return praiseId;
	}
	
	public void setUserId(Integer userId) {
		this.userId = userId;
	}
	
	public Integer getUserId(){
		return userId;
	}
	
	public void setPraiseContent(Integer praiseContent) {
		this.praiseContent = praiseContent;
	}
	
	public Integer getPraiseContent(){
		return praiseContent;
	}
	
	public void setContentType(Integer contentType){
		this.contentType = contentType;
	}
	
	public Integer getContentType(){
		return contentType;
	}
	
	public void setPraiseState(Integer praiseState) {
		this.praiseState = praiseState;
	}
	
	public Integer getPraiseState(){
		return praiseState;
	}
	
	public void setPraiseTime(Date praiseTime) {
		this.praiseTime = praiseTime;
	}
	
	@JsonFormat(pattern="yyyy-MM-dd HH:mm:ss",timezone = "GMT+8")
	public Date getPraiseTime(){
		return praiseTime;
	}

}
